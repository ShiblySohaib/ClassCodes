SELECT DISTINCT COUNTRY FROM EMPLOYEE_TABLE;
SELECT DISTINCT COUNTRY AS 'UNQ COUNTRY' FROM employee_table;
SELECT DISTINCT CITY AS 'UNQ CITY' FROM employee_table;
SELECT MAX(SALARY) AS 'MAX SALARY' FROM employee_table;
SELECT MIN(SALARY) AS 'MIN SALARY' FROM employee_table;
SELECT AVG(SALARY) AS 'AVG SALARY' FROM employee_table;
SELECT SUM(SALARY) AS 'SUM OF SALARY' FROM employee_table;
SELECT COUNT(SALARY) AS 'COUNT OF SALARY' FROM employee_table;
SELECT MAX(SALARY) AS 'MAX SALARY', MIN(SALARY) AS 'MIN SALARY' FROM employee_table;


