CREATE DATABASE LAB3;


CREATE TABLE EMPLOYEE_TABLE(
    ID INT(10) PRIMARY KEY AUTO_INCREMENT,
    NAME VARCHAR (50),
    CITY VARCHAR (50),
    COUNTRY VARCHAR (50),
    AGE INT (10),
    SALARY INT (10),
    EXPERIENCE INT (10),
    DEPARTMENT VARCHAR (50),
    DESIGNATION VARCHAR (50)
    );


